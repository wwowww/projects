{
  "compilerOptions": {
    "typeRoots" : ["./@types", "./node_modules/@types"],
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "types": ["vite-plugin-svgr/client"]
    },
    "skipLibCheck": true,
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext",
    // "module": "ESNext", 
    // "moduleResolution": "bundler" 
  },
  "include": ["**/*.ts", "**/*.tsx", "vite.config.ts", "src/prototypes/Zustand/ZustandTodoEditableItem", "src/svg.d.ts"],
  "exclude": ["node_modules"],
}
